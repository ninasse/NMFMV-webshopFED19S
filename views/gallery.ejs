<%- include ("header.ejs") %>

<h1>Produkter</h1>
<ul>
    <% productList.forEach(product => { %>
    <li>
        <div><%= product.name %></div>
        <div><%= product.album %></div>
        <div><%= product.description %></div>
        <div><%= product.price %></div>
        <div class="product-image-container"><img src="<%= product.description %>"></div>
    </li>
    <%}) %>
</ul>

<%- include ("footer.ejs") %>