<%- include ("header.ejs") %>
<%- include ("navbar.ejs") %>
<main class="adminpage">
    <h1 class="pageheadline">Inloggning för admin</h1>
    
    <form action="<%constant.ROUTE.loginAdmin%>" method="POST" class="admin-login-form">
        <fieldset>
            <input type="text" name="adminName" id="admin-name" placeholder="Namn" />
            <input type="password" name="adminPassword" id="admin-pw" placeholder="Lösenord" />
            <div class="submit-form">
                <button type="submit">Logga in</button>
            </div> 
        </fieldset>
    </form>

</main>
